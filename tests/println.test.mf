println(null);
println(true);
println(false);
println(123);
println(123.4);
println(Inf);
println(NaN);
println("foo");
println(Map{});
println(Set{});

let meta = type {
    "into_string": function(value) {
        return "ABC";
    }
};
let x = new meta Map{};
println(x);

let meta = type {
    "into_string": function(value) {
        return Map{};
    }
};
let y = new meta Map{};
try { println(y); } catch err { println($"error: {err}"); }

let meta = type {
    "into_string": function(value) {
        error "oopsie";
    }
};
let z = new meta Map{};
try { println(z); } catch err { println($"error: {err}"); }
