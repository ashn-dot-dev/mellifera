let a = {
    "abc",
    "def",
};
let b = {
    "foo",
    "bar",
};
dumpln(set::union(a, b));
dumpln(a.union(b));

let a = {
    "abc",
    "def",
};
let b = {
    "abc",
    "bar",
};
dumpln(set::union(a, b));
dumpln(a.union(b));

try { dumpln(set::union(123, Set{})); } catch err { dumpln(err); }
try { dumpln(set::union(Set{}, 123)); } catch err { dumpln(err); }
