let event = type {
    .party = function(self) {
        println("🥳 woo party 🎉");
    },
    .cleanup = function(self) {
        println("🧹 boo cleanup 🧽");
    },
};

let birthday = type extends(event, {
    .name = "birthday",
    .party = function(self) {
        println("🥳 woo birthday party 🎂");
    },
});

dumpln(birthday);
