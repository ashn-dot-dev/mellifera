"foobar" =~ r"((foo)(bar))";
dumpln(re::group(0));
dumpln(re::group(1));
dumpln(re::group(2));
dumpln(re::group(3));

print("\n");

"foobar" =~ r"((foo)(bar))";
try { dumpln(re::group(-1)); } catch err { dumpln(err); }
try { dumpln(re::group(+4)); } catch err { dumpln(err); }
try { dumpln(re::group(1.5)); } catch err { dumpln(err); }

print("\n");

"nope" =~ r"((foo)(bar))";
try { dumpln(re::group(0)); } catch err { dumpln(err); }
