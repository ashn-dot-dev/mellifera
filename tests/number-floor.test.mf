println($`(+123.45).floor() is {(+123.45).floor()}`);
println($`(-123.45).floor() is {(-123.45).floor()}`);
try { Inf.floor(); } catch err { println($`Inf.floor() produced the error {repr(err)}`); }
try { NaN.floor(); } catch err { println($`NaN.floor() produced the error {repr(err)}`); }
################################################################################
# (+123.45).floor() is 123
# (-123.45).floor() is -124
# Inf.floor() produced the error "cannot convert float infinity to integer"
# NaN.floor() produced the error "cannot convert float NaN to integer"
