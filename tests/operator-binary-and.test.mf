try { dumpln( Map{}        and Map{}        ); } catch err { dumpln(err); }
try { dumpln( Set{}        and Set{}        ); } catch err { dumpln(err); }
try { dumpln( 123.&        and 123.&        ); } catch err { dumpln(err); }
try { dumpln( function(){} and function(){} ); } catch err { dumpln(err); }
try { dumpln( println      and println      ); } catch err { dumpln(err); }

print("\n");

dumpln(false and false);
dumpln(false and true);
dumpln(true and false);
dumpln(true and true);

print("\n");

# short circuiting
let f = function(s) { println(s); error "oopsie"; };
false and f("false and f()");
try { true and f("true and f()"); } catch { println("error"); }
